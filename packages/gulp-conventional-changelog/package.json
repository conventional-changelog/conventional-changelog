{
  "name": "gulp-conventional-changelog",
  "version": "5.0.0",
  "description": "Generate a changelog using conventional-changelog.",
  "author": {
    "name": "Steve Mao",
    "email": "maochenyan@gmail.com",
    "url": "https://github.com/stevemao"
  },
  "license": "MIT",
  "homepage": "https://github.com/conventional-changelog/conventional-changelog/tree/master/packages/gulp-conventional-changelog#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/conventional-changelog/conventional-changelog.git",
    "directory": "packages/gulp-conventional-changelog"
  },
  "bugs": {
    "url": "https://github.com/conventional-changelog/conventional-changelog/issues"
  },
  "keywords": [
    "gulpplugin",
    "conventional-changelog",
    "conventional",
    "changelog",
    "log"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "directory": "package",
    "linkDirectory": false
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clear": "del ./package",
    "prepublishOnly": "run clear clean-publish",
    "postpublish": "pnpm clear"
  },
  "dependencies": {
    "add-stream": "^1.0.0",
    "concat-stream": "^2.0.0",
    "conventional-changelog": "workspace:^",
    "fancy-log": "^2.0.0",
    "plugin-error": "^2.0.0"
  }
}
